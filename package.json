{
	"name": "json-url",
	"version": "2.3.4",
	"description": "Compress JSON into compact base64 URI-friendly notation",
	"main": "dist/node/index.js",
	"browser": "dist/browser/json-url.js",
	"scripts": {
		"lint": "eslint src/*",
		"compile": "rm -rf dist/; mkdir dist & wait & npm run compile:node & npm run compile:webpack",
		"compile:node": "babel --plugins transform-runtime,add-module-exports -d dist/node --sourceRoot src src/main",
		"compile:webpack": "webpack",
		"prepublish": "npm test && npm run compile",
		"test": "npm run lint && npm run test:node && npm run test:karma",
		"test:node": "BABEL_ENV=test nyc mocha test/index.js --timeout 120000",
		"test:karma": "karma start --single-run",
		"perf": "babel-node test/perf.js",
		"coverage": "nyc report --reporter=text-lcov | coveralls",
		"example": "open http://localhost:8000/examples/browser/test.html & static-server --port 8000"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/masotime/json-url"
	},
	"author": "Benjamin Goh <masotime@gmail.com>",
	"license": "ISC",
	"dependencies": {
		"babel-runtime": "^6",
		"lz-string": "^1.4.4",
		"lzma": "^2.3.2",
		"msgpack5": "^3.5.1",
		"node-lzw": "^0.3.1",
		"urlsafe-base64": "^1.0.0",
		"bluebird": "^3.0.6"
	},
	"devDependencies": {
		"babel-cli": "^6",
		"babel-core": "^6.26.3",
		"babel-eslint": "^7",
		"babel-loader": "^7.1.2",
		"babel-plugin-add-module-exports": "^0",
		"babel-plugin-dynamic-import-node": "^1.1.0",
		"babel-plugin-istanbul": "^4.1.4",
		"babel-plugin-module-resolver": "^3.0.0-beta.5",
		"babel-plugin-syntax-dynamic-import": "^6.18.0",
		"babel-plugin-transform-runtime": "^6",
		"babel-polyfill": "^6.26.0",
		"babel-preset-es2015": "^6",
		"babel-preset-stage-0": "^6",
		"coveralls": "^3.0.2",
		"eslint": "^3",
		"eslint-import-resolver-babel-module": "^4.0.0",
		"eslint-plugin-import": "^2.14.0",
		"karma": "^3.1.1",
		"karma-chrome-launcher": "^2.2.0",
		"karma-mocha": "^1.3.0",
		"karma-webpack": "^3.0.5",
		"mocha": "^5.2.0",
		"nyc": "^13.1.0",
		"static-server": "^2.0.5",
		"uglifyjs-webpack-plugin": "^0.4.6",
		"webpack": "^3.5.6"
	}
}
